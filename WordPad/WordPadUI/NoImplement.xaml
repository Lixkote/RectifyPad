<ContentDialog
    x:Class="WordPad.WordPadUI.NoImplement"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:WordPad.WordPadUI"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="🚫 Sorry, we cannot do that yet"
    x:Uid="notimplementdialog"
    SecondaryButtonText="OK"
    Style="{StaticResource DefaultContentDialogStyle}"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick">

    <StackPanel>
        <TextBlock x:Uid="notimplement1" Text="This feature is not yet available." />
        <TextBlock x:Uid="notimplement2" Text="If you want, you can help to implement it by doing a pull request on GitHub." />
    </StackPanel>
</ContentDialog>
